<div class="container p-4">
    <div class="row">
    Preguntas del seminario
        <table class="default">
            <tr><th>Pregunta</th><th>De</th><th>Para</th><th>Puntos</th></tr>
        {{#each preguntas}}
            <tr><td>{{DS_CUESTION}}</td><td>{{DS_ORIGEN}}</td><td>{{DS_DESTINO}}</td><td>{{NM_VOTOS}}</td>
            {{#if VOTADO }}
                <td><a class="btn btn-secondary" disabled="disabled" href="">
                    Votado
                </a></td>
            {{else}}
                <td><a class="btn btn-secondary" href="preguntas/votar/{{CD_CUESTION}}">
                    Votar
                </a></td>
            {{/if}}
            </tr>
        {{/each}}
        </table>
    </div>
</div>

<div class="container p-4">
    <div class="row">
        <a class="btn btn-secondary" href="preguntas/actualizar">
            Actualizar
        </a>
    </div>
</div>